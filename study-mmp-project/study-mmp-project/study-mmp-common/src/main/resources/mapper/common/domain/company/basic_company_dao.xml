<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.common.domain.company.common_company_dao">

    <select id="selectCompanyByCompanyCd"  parameterType="String" resultType="study.mmp.common.domain.company.model.Company">
        SELECT
          *
        FROM
          company
        WHERE
          company_cd = #{companyCd}
    </select>
	
</mapper>